{
  "manifest_version": 3,
  "name": "Secret Manager",
  "version": "1.0",
  "description": "A Chrome extension to generate and store user secrets securely.",

  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "secretkey.png",
    "48": "secretkey.png",
    "128": "secretkey.png"
  },

  "action": {
    "default_popup": "index.html",
    "default_title": "Secret Manager Extension"
  }
}
